{
    "version": 2,
    "builds": [
      {
        "src": "./src/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/server.js"
      }
    ],
    "crons": [
      {
        "path": "/count/post",
        "schedule": "28 20 1-31 1-12 *"
      }
    ]
  }