  {
    "version": 2,
    "builds": [
      {
        "src": "./src/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/server.js"
      }
    ],
    "crons": [
      {
        "path": "/cron/gamification/drop",
        "schedule": "0 0 1 * *"
      }
    ]
  }